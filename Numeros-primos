#include <iostream>
using namespace std;

int main() {
    int numeros[10];

    // Solicitar al usuario que ingrese 10 números enteros
    cout << "Ingresa 10 numeros enteros:" << endl;
    for (int i = 0; i < 10; i++) {
        cout << "Numero " << i + 1 << ": ";
        cin >> numeros[i];
    }

    // Mostrar el arreglo original
    cout << "\nArreglo original:" << endl;
    for (int i = 0; i < 10; i++) {
        cout << numeros[i] << " ";
    }

    // Rotar a la derecha: el último pasa a ser el primero
    int ultimo = numeros[9];
    for (int i = 9; i > 0; i--) {
        numeros[i] = numeros[i - 1];
    }
    numeros[0] = ultimo;

    // Mostrar el arreglo rotado
    cout << "\n\nArreglo rotado una posicion a la derecha:" << endl;
    for (int i = 0; i < 10; i++) {
        cout << numeros[i] << " ";
    }

    cout << endl;
    return 0;
}
